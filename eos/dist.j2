{% extends "managed-full.j2" %}
{% block interfaces %}
{% for intf in downlinks %}
interface {{ intf.ifname }}
 switchport
 switchport mode trunk
 channel-group 1 mode active
 description DOWNLINK
{% endfor %}
{% endblock %}
{% block management %}
interface lo1
 no shutdown
 ip address {{ mgmt_ip }}/{{ mgmt_prefixlen }}
{% endblock %}
